<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
          PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
          
<mapper namespace="com.im.home.members.MembersMapper">

	<insert id="setMembersSiginUp" parameterType="MembersVO">
		INSRET INTO MEMBERS VALUES (#{id}, #{passWord}, #{userName}, #{email}, #{birth}, 1, "1234", SYSDATE());
	</insert>
	
	<select id="getMembersLogin" resultType="MembersVO">
		SELECT * FROM MEMBERS ORDER BY JOINDATE ASC;
	</select>
	
</mapper>